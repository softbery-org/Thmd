  <Style x:Key="ButtonStyle" TargetType="{x:Type Button}">
    <Setter Property="Background" Value="{StaticResource OrangeYellowGradient}" />
    <Setter Property="Width" Value="100" />
    <Setter Property="Height" Value="75" />
    <Setter Property="Margin" Value="0" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <Grid Width="{TemplateBinding FrameworkElement.Width}" Height="{TemplateBinding FrameworkElement.Height}" ClipToBounds="True">
            <Rectangle Name="outerRectangle" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" RadiusX="20" RadiusY="20" StrokeThickness="5" Fill="#00FFFFFF" Stroke="{StaticResource OrangeYellowGradient}" />
            <Rectangle Name="innerRectangle" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stroke="#00FFFFFF" StrokeThickness="20" RadiusX="20" RadiusY="20" Fill="{StaticResource OrangeYellowGradient}" />
