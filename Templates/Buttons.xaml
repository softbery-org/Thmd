  <Button x:Key="OrangeCloseButton">
    <ContentControl.Content>
      <ControlTemplate TargetType="{x:Type Button}">
        <FrameworkTemplate.Resources>
          <ResourceDictionary>
            <DrawingGroup x:Key="close_x" ClipGeometry="M0,0 L0,12 L12,12 L12,0 L0,0">
              <GeometryDrawing Brush="#FF000000" Geometry="F1 M12,12 M0,0 M2.22,2.22 A0.749,0.749 0 0 1 3.28,2.22L6,4.939 L8.72,2.22 A0.749,0.749 0 1 1 9.78,3.28L7.061,6 L9.78,8.72 A0.749,0.749 0 1 1 8.72,9.78L6,7.061 L3.28,9.78 A0.749,0.749 0 1 1 2.22,8.72L4.939,6 L2.22,3.28 A0.749,0.749 0 0 1 2.22,2.22" />
