<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="Thmd.Controls.ControlButtons.PlayerButtonsControlSecondRow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:Thmd.Controls.ControlButtons">
  <FrameworkElement.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Thmd;component/Templates/PlayerSecondButtons.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </FrameworkElement.Resources>
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>
    <Rectangle Grid.Column="0" Grid.ColumnSpan="15" Fill="#123341" Stroke="#000000" StrokeThickness="3" RadiusX="30" RadiusY="30" />
    <Button Name="_btnUpdate" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Update}" VerticalAlignment="Center" HorizontalAlignment="Center" />
    <Button Name="_btnSettings" Grid.Column="3" Grid.ColumnSpan="3" Style="{StaticResource Options}" VerticalAlignment="Center" HorizontalAlignment="Center" />
    <Button Name="_btnSubtitle" Grid.Column="6" Grid.ColumnSpan="3" Style="{StaticResource Subtitle}" VerticalAlignment="Center" HorizontalAlignment="Center" />
    <Button Name="_btnFullscreen" Grid.Column="9" Grid.ColumnSpan="3" Style="{StaticResource FullscreenOn}" VerticalAlignment="Center" HorizontalAlignment="Center" />
    <Button Name="_btnPlaylist" Grid.Column="12" Grid.ColumnSpan="3" Style="{StaticResource Playlist}" VerticalAlignment="Center" HorizontalAlignment="Center" />
  </Grid>
</UserControl>